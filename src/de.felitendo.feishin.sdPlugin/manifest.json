{
    "SDKVersion": 2,
    "Author": "Felitendo",
    "CodePath": "app.html",
    "Description": "Control Feishin, a modern self-hosted music player",
    "Name": "Feishin",
    "Icon": "actions/images/pluginIcon",
    "URL": "https://github.com/Felodeck/Feishin-Plugin",
    "Version": "1.0.0",
    "Software": {
        "MinimumVersion": "5.0"
    },
    "OS": [
        {
            "Platform": "mac",
            "MinimumVersion": "10.11"
        },
        {
            "Platform": "windows",
            "MinimumVersion": "10"
        }
    ],
    "Category": "Feishin",
    "CategoryIcon": "actions/images/categoryIcon",
    "Actions": [
        {
            "Icon": "actions/images/ppactionIcon",
            "Name": "Play/Pause",
            "States": [
                {
                    "Image": "actions/images/ppactionIcon"
                }
            ],
            "Tooltip": "Play or pause the current track",
            "UUID": "de.felitendo.feishin.playpause",
            "PropertyInspectorPath": "propertyinspector/index.html"
        },
        {
            "Icon": "actions/images/nextactionIcon",
            "Name": "Next",
            "States": [
                {
                    "Image": "actions/images/nextactionIcon"
                }
            ],
            "Tooltip": "Play the next track",
            "UUID": "de.felitendo.feishin.next",
            "PropertyInspectorPath": "propertyinspector/index.html"
        },
        {
            "Icon": "actions/images/previousactionIcon",
            "Name": "Previous",
            "States": [
                {
                    "Image": "actions/images/previousactionIcon"
                }
            ],
            "Tooltip": "Play the previous track",
            "UUID": "de.felitendo.feishin.previous",
            "PropertyInspectorPath": "propertyinspector/index.html"
        },
        {
            "Icon": "actions/images/shuffelactionIcon",
            "Name": "Shuffle",
            "States": [
                {
                    "Image": "actions/images/shuffelactionIcon"
                }
            ],
            "Tooltip": "Toggle shuffle mode",
            "UUID": "de.felitendo.feishin.shuffle",
            "PropertyInspectorPath": "propertyinspector/index.html"
        },
        {
            "Icon": "actions/images/repeatactionIcon",
            "Name": "Repeat",
            "States": [
                {
                    "Image": "actions/images/repeatactionIcon"
                }
            ],
            "Tooltip": "Toggle repeat mode",
            "UUID": "de.felitendo.feishin.repeat",
            "PropertyInspectorPath": "propertyinspector/index.html"
        }
    ]
}